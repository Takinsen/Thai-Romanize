{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r api/requirements.txt && python api/setup.py"
      }
    }
  ],
  "routes": [
    {
      "src": "/romanize",
      "methods": ["GET"],
      "dest": "/api/app.py"
    }
  ]
}